							<li><a href="/shoppingcart">Your Cart<span class="blue"><%=@shoppingcart_length%></span></a></li>
							<%if @shoppingcart_length!=0%>
							<li><a href="/checkout/new">Checkout</a></li>
							<%end%>

							<li><%=link_to"Logout",destroy_user_session_path,method: :delete%></li>
							<%else%>
